<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"

                tools:context=".MainActivity"
 >

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar" />


    <LinearLayout
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:orientation="vertical"
       android:layout_gravity="center_horizontal"
       android:layout_marginTop="30dp"



       >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="5dp"



        >

            <ImageView
            android:id="@+id/messmenu"
            android:layout_width="@dimen/landlogowidth"
            android:layout_height="@dimen/landlogoheight"
            android:contentDescription="@null"
            android:src="@drawable/messmenu"
            android:clickable="true"/>


            <ImageView
                android:id="@+id/citycinema"
                android:layout_width="@dimen/landlogowidth"
                android:layout_height="@dimen/landlogowidth"
                android:layout_marginLeft="@dimen/logosleftmargin"
                android:contentDescription="@null"
                android:src="@drawable/citycinema"
                android:clickable="true"/>

            <ImageView
                android:id="@+id/cmpusdeals"
                android:layout_width="@dimen/landlogowidth"
                android:layout_height="@dimen/landlogoheight"
                android:layout_marginLeft="@dimen/logosleftmargin"
                android:contentDescription="@null"
                android:src="@drawable/campusdeals"
                android:clickable="true"/>
        <ImageView
            android:id="@+id/campusupdates"
            android:layout_width="@dimen/landlogowidth"
            android:layout_height="@dimen/landlogoheight"
            android:layout_marginLeft="@dimen/logosleftmargin"
            android:contentDescription="@null"
            android:src="@drawable/campusupdates"
            android:clickable="true"/>


        <ImageView
            android:id="@+id/jaipurevents"
            android:layout_width="@dimen/landlogowidth"
            android:layout_height="@dimen/landlogoheight"
            android:layout_marginLeft="@dimen/logosleftmargin"
            android:contentDescription="@null"
            android:src="@drawable/jaipurevents"
            android:clickable="true"/>
    </LinearLayout>

     <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/textlogomargin"
         >
        <TextView
            android:id="@+id/mess"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"


            android:text="@string/messmenu"
            android:textColor="@color/blue2"

            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="25dp"
            android:text="@string/citycinema"
            android:textColor="@color/blue2"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="23dp"
            android:text="@string/campusdeals"
            android:textColor="@color/blue2"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="14dp"
            android:text="@string/campusupdates"
            android:textColor="@color/blue2"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="13dp"
            android:text="@string/jaipurevents"
            android:textColor="@color/blue2"
            />

        </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="8dp"
        >

        <ImageView
            android:id="@+id/photography"
            android:layout_width="@dimen/landlogowidth"
            android:layout_height="@dimen/landlogoheight"

            android:contentDescription="@null"
            android:src="@drawable/photography"
            android:clickable="true"/>
        <ImageView
            android:id="@+id/library"
            android:layout_width="@dimen/landlogowidth"
            android:layout_height="@dimen/landlogoheight"
            android:layout_marginLeft="@dimen/logosleftmargin"
            android:contentDescription="@null"
            android:src="@drawable/library"
            android:clickable="true"/>


       <ImageView
           android:id="@+id/pyp"
           android:layout_width="@dimen/landlogowidth"
           android:layout_height="@dimen/landlogoheight"
           android:layout_marginLeft="@dimen/logosleftmargin"
           android:contentDescription="@null"
           android:src="@drawable/pyp"
           android:clickable="true"/>

       <ImageView
           android:id="@+id/placementquiz"
           android:layout_width="@dimen/landlogowidth"
           android:layout_height="@dimen/landlogoheight"
           android:layout_marginLeft="@dimen/logosleftmargin"
           android:contentDescription="@null"
           android:src="@drawable/placementquiz"
           android:clickable="true"/>
        android:layout_marginTop="40dp"

        >




    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/textlogomargin"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"


            android:text="@string/photography"
            android:textColor="@color/blue2"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/blue2"
            android:text="@string/library"
            android:layout_marginLeft="40dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/blue2"
            android:layout_marginLeft="60dp"
            android:text="@string/pyp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/blue2"
            android:layout_marginLeft="45dp"
            android:text="@string/placementquiz"
            />

    </LinearLayout>

   </LinearLayout>
    </LinearLayout>


